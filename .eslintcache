n                            <Col md=\"7\">\r\n\r\n                                {rating && rating.star.map((st, index) => (\r\n                                    <Row>\r\n                                        <Col md=\"2\">{index + 1} star</Col>\r\n                                        <Col md=\"8\"> <Progress value={rating && (rating.star[index].count * 100) / rating.totalRating}></Progress> </Col>\r\n                                        <Col md=\"2\"> {Math.floor(rating && (rating.star[index].count * 100) / rating.totalRating)} %</Col>\r\n                                    </Row>\r\n                                ))}\r\n                            </Col>\r\n                        </Row>\r\n                    </CardBody>\r\n                </Card>\r\n            </div>\r\n        )\r\n    }\r\n}\r\n\r\nexport default View;","P:\\React\\e-shop\\src\\index.js",[],"P:\\React\\e-shop\\src\\reportWebVitals.js",[],"P:\\React\\e-shop\\src\\App.js",[],"P:\\React\\e-shop\\src\\shop\\index.jsx",["119"],"P:\\React\\e-shop\\src\\shop\\body\\index.jsx",["120"],"P:\\React\\e-shop\\src\\shop\\header\\index.jsx",["121"],"P:\\React\\e-shop\\src\\shop\\footer\\index.jsx",[],"P:\\React\\e-shop\\src\\shop\\router\\router.jsx",[],"P:\\React\\e-shop\\src\\shop\\header\\navbar.jsx",["122"],"P:\\React\\e-shop\\src\\shop\\view\\view.jsx",[],"P:\\React\\e-shop\\src\\shop\\body\\right\\index.jsx",["123"],"P:\\React\\e-shop\\src\\shop\\body\\left\\index.jsx",[],"P:\\react\\e-shop\\src\\shop\\cart\\index.jsx",["124"],{"ruleId":"125","replacedBy":"126"},{"ruleId":"127","replacedBy":"128"},{"ruleId":"129","severity":1,"message":"130","line":5,"column":8,"nodeType":"131","messageId":"132","endLine":5,"endColumn":12},{"ruleId":"129","severity":1,"message":"133","line":1,"column":17,"nodeType":"131","messageId":"132","endLine":1,"endColumn":26},{"ruleId":"134","severity":1,"message":"135","line":20,"column":34,"nodeType":"136","messageId":"137","endLine":20,"endColumn":36},{"ruleId":